<p id="notice"><%= notice %></p>

<h1>Scrapers</h1>

<table class="table">
  <thead>
    <tr>
      <th>Thread size</th>
      <th>Proxy usage</th>
      <th>Http method</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @scrapers.each do |scraper| %>
      <tr>
        <td><%= scraper.thread_size %></td>
        <td><%= scraper.proxy_usage %></td>
        <td><%= scraper.http_method %></td>
        <td><%= link_to 'Show', scraper %></td>
        <td><%= link_to 'Edit', edit_scraper_path(scraper) %></td>
        <td><%= link_to 'Destroy', scraper, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Scraper', new_scraper_path, class: 'btn btn-primary'%>
